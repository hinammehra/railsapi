    <div>
        <table class="table table-striped table-bordered table-hover table-responsive" style="margin-top: -10px;">
            <tr class="info">
                <th></th>
                <th>#</th>
                <th>Date Created</th>
                <th>Total</th>
                <th>Remaining Credit</th>
                <th>Reference</th>
            </tr>
            <% cn_op.keys.each do |co| %>
            <tr>
                <td><%= check_box_tag 'cn_op[]', [cn_op[co][:status], cn_op[co][:date].to_date, cn_op[co][:remaining_credit]] %></td>
                <td><%= co %></td>
                <td><%= cn_op[co][:date].to_date.strftime("%d/%-m/%y") %></td>
                <td><%= cn_op[co][:total] %></td>
                <td><%= cn_op[co][:remaining_credit] %></td>
                <td><%= cn_op[co][:reference] %></td>
            </tr>
            <% end%>
            <!--  customer.xero_contact.xero_invoices.has_amount_due.each do |i|-->
        </table>
    </div>
