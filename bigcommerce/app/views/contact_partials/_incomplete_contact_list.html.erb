<table class="table table-striped table-bordered table-hover table-responsive w-100 d-block d-table mt-0 mx-3">
	<tr>
		<th>Contact</th>
		<th>Phone</th>
	</tr>

	<% contacts.each do |c| %>
		<tr>
			<td>
				<%= link_to c.name, controller: 'contact', action: 'summary', contact_id: c.id %>
			</td>
			<td>
				<%= c.personal_number %>
			</td>
		</tr>
	<% end %>
</table>
